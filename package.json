{
  "name": "@lucaedu-2/luca-ds",
  "version": "1.11.0",
  "description": "A modern UI component library built with React, TypeScript, and Tailwind CSS",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./styles.css": "./dist/luca-ds.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "watch:css": "tailwindcss -c tailwind.config.js -i src/styles.css -o dist/luca-ds.css --watch",
    "storybook": "storybook dev -p 6006 --initial-path=/story/introduction-welcome--home",
    "build-storybook": "storybook build -o docs",
    "lint": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luca-edu/luca-ds.git"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwind",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luca-edu/luca-ds/issues"
  },
  "homepage": "https://github.com/luca-edu/luca-ds#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@heroicons/react": "^2.2.0",
    "antd": "^5.28.0",
    "antd-style": "^3.7.1",
    "clsx": "^2.1.1",
    "rc-virtual-list": "^3.11.1"
  }
}
